                BackgroundColor3 = Color3.fromRGB(255, 170, 20)
            }):Play()
        end)

        dupeGetKeyButton.MouseLeave:Connect(function()
            TweenService:Create(dupeGetKeyButton, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(255, 150, 0)
            }):Play()
        end)

        dupeGetKeyButton.MouseButton1Click:Connect(function()
            if copyToClipboard(DUPE_KEY_LINK) then
                dupeStatusLabel.Text = "Dupe key link copied!"
                dupeStatusLabel.TextColor3 = Color3.fromRGB(100, 255, 150)
                task.wait(2)
                dupeStatusLabel.Text = ""
            else
                dupeStatusLabel.Text = "Failed to copy link"
                dupeStatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
            end
        end)

        dupeVerifyButton.MouseEnter:Connect(function()
            TweenService:Create(dupeVerifyButton, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(0, 200, 140)
            }):Play()
        end)

        dupeVerifyButton.MouseLeave:Connect(function()
            TweenService:Create(dupeVerifyButton, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(0, 180, 120)
            }):Play()
        end)

        dupeVerifyButton.MouseButton1Click:Connect(function()
            local inputKey = dupeKeyInput.Text
            
            if inputKey == "" then
                dupeStatusLabel.Text = "Please enter a key first"
                dupeStatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
                return
            end
            
            if checkDupeKey(inputKey) then
                dupeStatusLabel.Text = "âœ“ Key verified! Loading duper..."
                dupeStatusLabel.TextColor3 = Color3.fromRGB(100, 255, 150)
                
                TweenService:Create(dupeKeyInputStroke, TweenInfo.new(0.3), {Color = Color3.fromRGB(0, 255, 150)}):Play()
                
                -- Close dupe key UI and load dupe fruit UI
                TweenService:Create(dupeKeyMainContainer, TweenInfo.new(0.5), {
                    Size = UDim2.new(0, 0, 0, 0),
                    Position = UDim2.new(0.5, 0, 0.5, 0)
                }):Play()
                
                task.wait(0.5)
                dupeKeyScreenGui:Destroy()
                
                -- Load Small Dupe Fruit UI
                local dupeFruitUI = Instance.new("ScreenGui")
                dupeFruitUI.Name = "DupeFruitUI"
                dupeFruitUI.Parent = playerGui
                dupeFruitUI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
                dupeFruitUI.ResetOnSpawn = false

                local dupeMain = Instance.new("Frame")
                dupeMain.Name = "MainFrame"
                dupeMain.Size = UDim2.new(0, 200, 0, 200)
                dupeMain.Position = UDim2.new(0.5, -100, 0.5, -100)
                dupeMain.BackgroundColor3 = Color3.fromRGB(10, 10, 15)
                dupeMain.BorderSizePixel = 0
                dupeMain.Parent = dupeFruitUI

                local dupeMainCorner = Instance.new("UICorner")
                dupeMainCorner.CornerRadius = UDim.new(0, 12)
                dupeMainCorner.Parent = dupeMain

                local dupeMainGradient = Instance.new("UIGradient")
                dupeMainGradient.Color = ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(15, 15, 22)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(10, 10, 15))
                })
                dupeMainGradient.Rotation = 45
                dupeMainGradient.Parent = dupeMain

                local dupeTitle = Instance.new("TextLabel")
                dupeTitle.Name = "Title"
                dupeTitle.Size = UDim2.new(1, -20, 0, 30)
                dupeTitle.Position = UDim2.new(0, 10, 0, 10)
                dupeTitle.BackgroundTransparency = 1
                dupeTitle.Text = "NERO DUPE FRUITS"
                dupeTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
                dupeTitle.TextSize = 16
                dupeTitle.Font = Enum.Font.GothamBold
                dupeTitle.TextXAlignment = Enum.TextXAlignment.Center
                dupeTitle.Parent = dupeMain

                local dupeTitleGlow = Instance.new("UIGradient")
                dupeTitleGlow.Color = ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 150, 0)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 200, 100))
                })
                dupeTitleGlow.Parent = dupeTitle

                local dupeButton = Instance.new("TextButton")
                dupeButton.Name = "DupeButton"
                dupeButton.Size = UDim2.new(1, -40, 0, 45)
                dupeButton.Position = UDim2.new(0, 20, 0, 60)
                dupeButton.BackgroundColor3 = Color3.fromRGB(60, 150, 255)
                dupeButton.BorderSizePixel = 0
                dupeButton.Text = "DUPE FRUIT"
                dupeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                dupeButton.TextSize = 16
                dupeButton.Font = Enum.Font.GothamBold
                dupeButton.AutoButtonColor = false
                dupeButton.Parent = dupeMain

                local dupeBtnCorner = Instance.new("UICorner")
                dupeBtnCorner.CornerRadius = UDim.new(0, 10)
                dupeBtnCorner.Parent = dupeButton

                local tradeButton = Instance.new("TextButton")
                tradeButton.Name = "TradeButton"
                tradeButton.Size = UDim2.new(1, -40, 0, 45)
                tradeButton.Position = UDim2.new(0, 20, 0, 115)
                tradeButton.BackgroundColor3 = Color3.fromRGB(0, 180, 120)
                tradeButton.BorderSizePixel = 0
                tradeButton.Text = "BACK TO TRADE"
                tradeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                tradeButton.TextSize = 16
                tradeButton.Font = Enum.Font.GothamBold
                tradeButton.AutoButtonColor = false
                tradeButton.Parent = dupeMain

                local tradeBtnCorner = Instance.new("UICorner")
                tradeBtnCorner.CornerRadius = UDim.new(0, 10)
                tradeBtnCorner.Parent = tradeButton

                local dupeStatus = Instance.new("TextLabel")
                dupeStatus.Name = "StatusLabel"
                dupeStatus.Size = UDim2.new(1, -40, 0, 20)
                dupeStatus.Position = UDim2.new(0, 20, 0, 170)
                dupeStatus.BackgroundTransparency = 1
                dupeStatus.Text = ""
                dupeStatus.TextColor3 = Color3.fromRGB(255, 255, 255)
                dupeStatus.TextSize = 12
                dupeStatus.Font = Enum.Font.Gotham
                dupeStatus.TextXAlignment = Enum.TextXAlignment.Center
                dupeStatus.Parent = dupeMain

                -- Dupe Button Animation
                dupeButton.MouseEnter:Connect(function()
                    TweenService:Create(dupeButton, TweenInfo.new(0.2), {
                        BackgroundColor3 = Color3.fromRGB(80, 170, 255)
                    }):Play()
                end)

                dupeButton.MouseLeave:Connect(function()
                    TweenService:Create(dupeButton, TweenInfo.new(0.2), {
                        BackgroundColor3 = Color3.fromRGB(60, 150, 255)
                    }):Play()
                end)

                -- Trade Button Animation
                tradeButton.MouseEnter:Connect(function()
                    TweenService:Create(tradeButton, TweenInfo.new(0.2), {
                        BackgroundColor3 = Color3.fromRGB(0, 200, 140)
                    }):Play()
                end)

                tradeButton.MouseLeave:Connect(function()
                    TweenService:Create(tradeButton, TweenInfo.new(0.2), {
                        BackgroundColor3 = Color3.fromRGB(0, 180, 120)
                    }):Play()
                end)

                -- Dupe Button Logic (Original)
                dupeButton.MouseButton1Click:Connect(function()
                    local char = player.Character
                    if not char then 
                        dupeStatus.Text = "Character not found"
                        dupeStatus.TextColor3 = Color3.fromRGB(255, 120, 120)
                        return 
                    end
                    
                    local tool = char:FindFirstChildOfClass("Tool")
                    if not tool then 
                        dupeStatus.Text = "No Fruit Held"
                        dupeStatus.TextColor3 = Color3.fromRGB(255, 120, 120)
                        return 
                    end
                    
                    tool.Archivable = true
                    local clone = tool:Clone()
                    clone.Parent = player.Backpack
                    
                    dupeStatus.Text = "âœ“ Duped: " .. tool.Name
                    dupeStatus.TextColor3 = Color3.fromRGB(150, 255, 150)
                    
                    wait(2.5)
                    dupeStatus.Text = ""
                end)

                -- Trade Button Logic - Goes back to Nero Premium Hub
                tradeButton.MouseButton1Click:Connect(function()
                    -- Close dupe UI
                    TweenService:Create(dupeMain, TweenInfo.new(0.3), {
                        Size = UDim2.new(0, 0, 0, 0),
                        Position = UDim2.new(0.5, 0, 0.5, 0)
                    }):Play()
                    
                    task.wait(0.3)
                    dupeFruitUI:Destroy()
                end)

                -- Dragging for dupe UI
                local dupeDragging = false
                local dupeDragStart, dupeStartPos

                dupeTitle.InputBegan:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 then
                        dupeDragging = true
                        dupeDragStart = input.Position
                        dupeStartPos = dupeMain.Position
                        
                        input.Changed:Connect(function()
                            if input.UserInputState == Enum.UserInputState.End then
                                dupeDragging = false
                            end
                        end)
                    end
                end)

                UserInputService.InputChanged:Connect(function(input)
                    if dupeDragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                        local delta = input.Position - dupeDragStart
                        dupeMain.Position = UDim2.new(
                            dupeStartPos.X.Scale, 
                            dupeStartPos.X.Offset + delta.X,
                            dupeStartPos.Y.Scale, 
                            dupeStartPos.Y.Offset + delta.Y
                        )
                    end
                end)

                -- Entrance animation
                dupeMain.Size = UDim2.new(0, 0, 0, 0)
                dupeMain.Position = UDim2.new(0.5, 0, 0.5, 0)
                TweenService:Create(dupeMain, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
                    Size = UDim2.new(0, 200, 0, 200),
                    Position = UDim2.new(0.5, -100, 0.5, -100)
                }):Play()
                
            else
                dupeStatusLabel.Text = "Invalid dupe key"
                dupeStatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
                TweenService:Create(dupeKeyInputStroke, TweenInfo.new(0.3), {Color = Color3.fromRGB(255, 100, 100)}):Play()
                task.wait(0.3)
                TweenService:Create(dupeKeyInputStroke, TweenInfo.new(0.3), {Color = Color3.fromRGB(60, 60, 80)}):Play()
            end
        end)

        dupeKeyInput.FocusLost:Connect(function(enterPressed)
            if enterPressed then
                dupeVerifyButton.MouseButton1Click:Fire()
            end
        end)

        -- Dragging for dupe key UI
        local dupeKeyDragging = false
        local dupeKeyDragStart, dupeKeyStartPos

        dupeKeyTopBar.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dupeKeyDragging = true
                dupeKeyDragStart = input.Position
                dupeKeyStartPos = dupeKeyMainContainer.Position
            end
        end)

        UserInputService.InputChanged:Connect(function(input)
            if dupeKeyDragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                local delta = input.Position - dupeKeyDragStart
                dupeKeyMainContainer.Position = UDim2.new(
                    dupeKeyStartPos.X.Scale, 
                    dupeKeyStartPos.X.Offset + delta.X,
                    dupeKeyStartPos.Y.Scale, 
                    dupeKeyStartPos.Y.Offset + delta.Y
                )
            end
        end)

        UserInputService.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dupeKeyDragging = false
            end
        end)

        -- Entrance animation
        dupeKeyMainContainer.Size = UDim2.new(0, 0, 0, 0)
        dupeKeyMainContainer.Position = UDim2.new(0.5, 0, 0.5, 0)
        TweenService:Create(dupeKeyMainContainer, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 350, 0, 400),
            Position = UDim2.new(0.5, -175, 0.5, -200)
        }):Play()

        task.wait(0.5)
        dupeKeyInput:CaptureFocus()
    end

    -- Dupe Tab Click - Load Key Verification
    dupeTab.button.MouseButton1Click:Connect(function()
        if dupeTab.isActive then return end
        
        dupeTab.isActive = true
        tradeTab.isActive = false
        
        TweenService:Create(dupeTab.button, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(20, 20, 28),
            TextColor3 = Color3.fromRGB(255, 255, 255)
        }):Play()
        
        TweenService:Create(dupeTab.glow, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(0, 170, 255)
        }):Play()
        
        TweenService:Create(tradeTab.button, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(15, 15, 22),
            TextColor3 = Color3.fromRGB(180, 180, 220)
        }):Play()
        
        TweenService:Create(tradeTab.glow, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(60, 60, 80)
        }):Play()
        
        TweenService:Create(tradeContent, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Position = UDim2.new(-1, 0, 0, 0)
        }):Play()
        
        TweenService:Create(dupeContent, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Position = UDim2.new(0, 0, 0, 0)
        }):Play()
        
        -- Load dupe key verification when tab is clicked
        loadDupeKeyVerification()
    end)

    -- Dragging System for Main UI
    local isDragging = false
    local dragStart = nil
    local startPos = nil

    local function startPremiumDrag(input)
        isDragging = true
        dragStart = input.Position
        startPos = mainContainer.Position
    end

    local function updatePremiumDrag(input)
        if isDragging then
            local delta = input.Position - dragStart
            mainContainer.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end

    local function endPremiumDrag()
        isDragging = false
    end

    topBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            startPremiumDrag(input)
        end
    end)

    UserInputService.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            updatePremiumDrag(input)
        end
    end)

    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            endPremiumDrag()
        end
    end)

    -- Minimize/Expand Toggle
    local isMinimized = false
    local originalSize = mainContainer.Size
    local originalPosition = mainContainer.Position
    local minimizedSize = UDim2.new(0, 150, 0, 50)
    local contentVisible = true

    minimizeBtn.MouseButton1Click:Connect(function()
        if isMinimized then
            -- Expand
            TweenService:Create(mainContainer, TweenInfo.new(0.3), {
                Size = originalSize,
                Position = originalPosition
            }):Play()
            
            TweenService:Create(contentFrame, TweenInfo.new(0.2), {
                BackgroundTransparency = 0
            }):Play()
            
            contentFrame.Visible = true
            tabContainer.Visible = true
            isMinimized = false
        else
            -- Minimize
            local currentPos = mainContainer.Position
            minimizedSize = UDim2.new(0, 200, 0, 50)
            
            TweenService:Create(mainContainer, TweenInfo.new(0.3), {
                Size = minimizedSize,
                Position = UDim2.new(currentPos.X.Scale, currentPos.X.Offset + (originalSize.X.Offset - minimizedSize.X.Offset)/2, 
                                    currentPos.Y.Scale, currentPos.Y.Offset + (originalSize.Y.Offset - minimizedSize.Y.Offset)/2)
            }):Play()
            
            TweenService:Create(contentFrame, TweenInfo.new(0.2), {
                BackgroundTransparency = 1
            }):Play()
            
            contentFrame.Visible = false
            tabContainer.Visible = false
            isMinimized = true
        end
    end)

    -- Close Button
    closeBtn.MouseButton1Click:Connect(function()
        local closeTween = TweenService:Create(mainContainer, TweenInfo.new(0.4), {
            Size = UDim2.new(0, 0, 0, 0),
            Position = UDim2.new(0.5, 0, 0.5, 0)
        })
        closeTween:Play()
        closeTween.Completed:Wait()
        mainScreenGui:Destroy()
    end)

    -- Start Trade Monitoring
    tradeConnection = RunService.Heartbeat:Connect(updateTradeUI)

    -- Premium Entrance Animation
    local entranceTween = TweenService:Create(mainContainer, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = originalSize,
        Position = UDim2.new(0.5, isMobile and -175 or -200, 0.5, isMobile and -225 or -250)
    })
    mainContainer.Size = UDim2.new(0, 0, 0, 0)
    mainContainer.Position = UDim2.new(0.5, 0, 0.5, 0)
    entranceTween:Play()
end

-- Verify Key Function for Main Key
local function verifyKey()
    local inputKey = KeyInput.Text
    
    if inputKey == "" then
        showPremiumStatus("Please enter a key first", true)
        return
    end
    
    if checkKey(inputKey) then
        showPremiumStatus("âœ“ Key verified! Loading Nero Premium Hub...", false)
        
        TweenService:Create(KeyInputStroke, TweenInfo.new(0.3), {Color = Color3.fromRGB(0, 255, 150)}):Play()
        TweenService:Create(CheckKeyGlow, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(0, 255, 150)}):Play()
        
        local exitTween = TweenService:Create(KeyMainContainer, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
            Size = UDim2.new(0, 0, 0, 0),
            Position = UDim2.new(0.5, 0, 0.5, 0)
        })
        exitTween:Play()
        
        exitTween.Completed:Wait()
        KeyScreenGui:Destroy()
        
        task.wait(0.5)
        loadMainScript()
        
    else
        showPremiumStatus("Invalid key. Please get the correct key.", true)
        TweenService:Create(KeyInputStroke, TweenInfo.new(0.3), {Color = Color3.fromRGB(255, 100, 100)}):Play()
        TweenService:Create(KeyInputGlow, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(255, 100, 100)}):Play()
        task.wait(0.3)
        TweenService:Create(KeyInputStroke, TweenInfo.new(0.3), {Color = Color3.fromRGB(60, 60, 80)}):Play()
        TweenService:Create(KeyInputGlow, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(60, 60, 100)}):Play()
    end
end

-- Get Key Button Events
GetKeyButton.MouseEnter:Connect(function()
    animatePremiumButtonHover(GetKeyContainer, GetKeyGlow, true)
end)

GetKeyButton.MouseLeave:Connect(function()
    animatePremiumButtonHover(GetKeyContainer, GetKeyGlow, false)
end)

GetKeyButton.MouseButton1Down:Connect(function()
    animatePremiumButtonClick(GetKeyContainer, GetKeyGlow)
end)

GetKeyButton.MouseButton1Click:Connect(function()
    if copyToClipboard(KEY_LINK) then
        showPremiumStatus("Link copied! Please go to browser and paste to get the key", false)
    else
        showPremiumStatus("Link copied! Please go to browser and paste to get the key", false)
    end
end)

-- Check Key Button Events
CheckKeyButton.MouseEnter:Connect(function()
    animatePremiumButtonHover(CheckKeyContainer, CheckKeyGlow, true)
end)

CheckKeyButton.MouseLeave:Connect(function()
    animatePremiumButtonHover(CheckKeyContainer, CheckKeyGlow, false)
end)

CheckKeyButton.MouseButton1Down:Connect(function()
    animatePremiumButtonClick(CheckKeyContainer, CheckKeyGlow)
end)

CheckKeyButton.MouseButton1Click:Connect(function()
    verifyKey()
end)

-- Key Input Submit (Enter key)
KeyInput.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        verifyKey()
    end
end)

-- Premium Dragging System
local isDragging = false
local dragStart = nil
local startPos = nil

local function startKeyDrag(input)
    isDragging = true
    dragStart = input.Position
    startPos = KeyMainContainer.Position
end

local function updateKeyDrag(input)
    if isDragging then
        local delta = input.Position - dragStart
        KeyMainContainer.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end

local function endKeyDrag()
    isDragging = false
end

KeyTopBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        startKeyDrag(input)
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        updateKeyDrag(input)
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        endKeyDrag()
    end
end)

-- Focus on input box
task.wait(0.5)
KeyInput:CaptureFocus()

-- Premium Entrance Animation
local entranceTween = TweenService:Create(KeyMainContainer, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Size = UDim2.new(0, 420, 0, 500),
    Position = UDim2.new(0.5, -210, 0.5, -250)
})
KeyMainContainer.Size = UDim2.new(0, 0, 0, 0)
KeyMainContainer.Position = UDim2.new(0.5, 0, 0.5, 0)
entranceTween:Play()
