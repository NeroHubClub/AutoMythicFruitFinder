        dupeKeyInput.PlaceholderColor3 = Color3.fromRGB(100, 100, 130)
        dupeKeyInput.Text = ""
        dupeKeyInput.TextColor3 = Color3.fromRGB(255, 255, 255)
        dupeKeyInput.TextSize = 16
        dupeKeyInput.Font = Enum.Font.Gotham
        dupeKeyInput.ClearTextOnFocus = false
        dupeKeyInput.Parent = dupeKeyMainFrame

